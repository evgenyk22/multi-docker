{
  "AWSEBDockerrunVersion": 2,
  
  "containerDefinitions": [
  {
      "name": "client",
      "image":  "evgenik22/multi-client",
      "hostname": "client",
      "essential": "false"
  },
  {
      "name": "api",
      "image":  "evgenik22/multi-server",
      "hostname": "api",
      "essential": "false"
  },
  {
      "name": "worker",
      "image":  "evgenik22/multi-worker",
      "hostname": "worker",
      "essential": "false"
  } ,
  {
      "name": "nginx",
      "image":  "evgenik22/multi-nginx",
      "hostname": "nginx",
      "essential": "true",
      "portMappings": [
          {
          "hostPort": 80,
          "containerPort": 80
          }
      ],
      "links": ["client","server"]
  }
  ]
}
